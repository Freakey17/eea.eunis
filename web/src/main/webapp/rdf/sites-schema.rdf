<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	 xmlns:owl="http://www.w3.org/2002/07/owl#"
	 xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xml:lang="en">

	<owl:Ontology rdf:about="">
		<rdfs:label>Sites schema</rdfs:label>
		<owl:versionInfo>$Id$</owl:versionInfo>
	</owl:Ontology>

	<rdfs:Class rdf:ID="Site">
		<rdfs:label>EUNIS Site</rdfs:label>
	</rdfs:Class>

	<rdfs:Class rdf:ID="Designation">
		<rdfs:label>EUNIS Designation</rdfs:label>
	</rdfs:Class>

<!-- Properties -->
	<rdf:Property rdf:ID="character">
		<rdfs:label>Character</rdfs:label>
		<rdfs:domain rdf:resource="#Site"/>
	</rdf:Property>

	<rdf:Property rdf:ID="designation">
		<rdfs:label>Designation</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="hasDesignation">
		<rdfs:label>Designation</rdfs:label>
		<rdfs:range rdf:resource="#Designation"/>
	</rdf:Property>

	<rdf:Property rdf:ID="idSite">
		<rdfs:label>Site ID</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="manager">
		<rdfs:label>Manager</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="name">
		<rdfs:label>Name</rdfs:label>
		<rdfs:domain rdf:resource="#Site"/>
		<rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
	</rdf:Property>

	<rdf:Property rdf:ID="respondent">
		<rdfs:label>Respondent</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="complexName">
		<rdfs:label>Complex name</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="districtName">
		<rdfs:label>District name</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="ownership">
		<rdfs:label>Ownership</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="history">
		<rdfs:label>History</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="description">
		<rdfs:label>Description</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="managementPlan">
		<rdfs:label>Management plan</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="iucnat">
		<rdfs:label>Region code</rdfs:label>
		<rdfs:comment>Administrative region code (IUCN) - only for CDDA data</rdfs:comment>
	</rdf:Property>

	<rdf:Property rdf:ID="designationDate">
		<rdfs:label>Designation date</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="compilationDate">
		<rdfs:label>Site compilation date</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="proposedDate">
		<rdfs:label>Date site proposed as eligible as SCI</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="confirmedDate">
		<rdfs:label>Date confirmed as SCI</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="updateDate">
		<rdfs:label>Date information last updated</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="spaDate">
		<rdfs:label>Date site classified as SPA</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="sacDate">
		<rdfs:label>Date site classified as SAC</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="nationalCode">
		<rdfs:label>National code</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="natura_2000">
		<rdfs:label>Natura 2000</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="nuts">
		<rdfs:label>NUTS code</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="area">
		<rdfs:label>Area (ha)</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="length">
		<rdfs:label>Length (m)</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="altMean">
		<rdfs:label>Mean altitude (m)</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="altMax">
		<rdfs:label>Max altitude (m)</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="altMin">
		<rdfs:label>Min altitude (m)</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="longitude">
		<rdfs:label>Longitude</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="latitude">
		<rdfs:label>Latitude</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="category"> <!-- used? -->
		<rdfs:label>Category</rdfs:label>
	</rdf:Property>

	<rdf:Property rdf:ID="sourceDb">
		<rdfs:label>Source database</rdfs:label>
	</rdf:Property>
</rdf:RDF>
